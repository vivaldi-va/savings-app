doctype html
html(lang='en')
	head
		title= 'log in'

	body
		form(action="/api/auth/login", method="post")
			div
				label(for="email") email
				input(type="email", name="email", id="email")
			div
				label(for="password") password
				input(type="password", name="password", id="password")
			div
				span #{error}
				button(type="submit") log in
